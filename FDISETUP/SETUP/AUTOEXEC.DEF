@ECHO OFF
SET LANG=$FLANG$
SET MTCPCFG=%dosdir%\MTCP.CFG
SET WATTCP.CFG=%dosdir%
SET PATH=%dosdir%\BIN
SET PATH=%path%;C:\TOOLS
SET NLSPATH=%dosdir%\NLS
SET HELPPATH=%dosdir%\HELP
SET TEMP=%dosdir%\TEMP
SET TMP=%TEMP%
SET BLASTER=A220 I5 D1 H5 P330
SET DIRCMD=/P /OGN /4
SET COPYCMD=/-Y
if "%config%"=="4" goto end
LH FDAPM APMDOS
IF "%config%"=="2" LH SHARE
LH DOSLFN
REM NLSFUNC %dosdir%\BIN\COUNTRY.SYS
REM DISPLAY CON=(EGA),858,2)
REM MODE CON CP PREP=((858) %dosdir%\CPI\EGA.CPX)
REM KEYB US,858,%dosdir%\bin\keyboard.sys
REM CHCP 858
REM LH PCNTPK INT=0x60
REM DHCP
REM MOUSE
CTMOUSE

SHSUCDX /QQ /D3
REM LH SHSUCDHD /QQ /F:FDBOOTCD.ISO
REM DEVLOAD /H /Q %dosdir%\BIN\UIDE.SYS /H /D:FDCD0001 /S5
SHSUCDX /QQ /~ /D:?SHSU-CDR,D /D:?SHSU-CDH,D /D:?FDCD0001,D /D:?FDCD0002,D /D:?FDCD0003,D

MEM /C /N
IF NOT "%config%"=="4" SHSUCDX /D
GOTO END
:END
SET AUTOFILE=%0
SET CFGFILE=$FDRIVE$\FDCONFIG.SYS
alias reboot=fdapm warmboot
alias reset=fdisk /reboot
alias halt=fdapm poweroff
alias shutdown=fdapm poweroff
alias cfg=edit %cfgfile%
alias auto=edit %0

set OS_NAME=FreeDOS
set OS_VERSION=$OSVER$

if exist %dosdir%\V8POWER\NUL goto V8Power

echo Done processing startup files %cfgfile% and %0
echo Type HELP to get support on commands and navigation
echo.
echo  Welcome to the %OS_NAME% %OS_VERSION% operating system (http://www.freedos.org)
goto DONE

***** V8Power Language specific text data.
English (EN)
DONE.EN=Done processing startup files /fCyan %1 /a7 and /fCyan %2 /a7
HELP.EN=Type /fWhite %1 /fGray to get support on commands and navigation.
WELCOME.EN=Welcome to the /fGreen %1 /fCyan %2 /fGray operating system ( /s- /fYellow %3 /fGray )

Spanish (ES)
DONE.ES=Inicio transformaci¢n efectuada archivos /fCyan %1 /a7 y /fCyan %2 /a7
HELP.ES=Escriba /fWhite %1 /fGray para conseguir el apoyo de los comandos y de navegaci¢n.
WELCOME.ES=Bienvenido al sistema operativo /fGreen %1 /fCyan %2 /fGray ( /s- /fYellow "%3" /fGray )

:V8Power
set PATH=%PATH%;%dosdir%\V8POWER
set DEFLANG=EN
if "%LANG%" == "" goto NoLanguage
if %LANG% == ES set DEFLANG=ES
:NoLanguage
vecho /p /t %0 DONE.%DEFLANG% %cfgfile% %0
vecho /p /t %0 HELP.%DEFLANG% HELP
vecho /p /t %0 WELCOME.%DEFLANG% %OS_NAME% %OS_VERSION% http://www.freedos.org
vecho
SET DEFLANG=

:DONE
