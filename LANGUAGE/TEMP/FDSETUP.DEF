INSTALL_OPTS=/w46 /h4 /c
INSTALL=/s- "Prˆt pour l'installation " /f %1 "%2" /f %3 .
INSTALL?="Voulez-vous intaller maintenant?"
INSTALL_YES="  Oui - Installer %1."
; Also uses EXIT defined above.
